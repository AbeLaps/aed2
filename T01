#include <stdio.h>
#include <time.h>
#include <stdlib.h>

void preenche_vetor_aleatorio(int v[],int tam){
    int k;
    for(int i = 0; i < tam; i++){
        k = rand();
        v[i] = k;
    }
    return;
}

int busca_bin(int v[],int tam,int val){
    return 0;
}

int busca_seq(int v[], int tam, int val){
    for(int i = 0;i < tam; i++){
        if(val == v[i]){
            return 1;
        }
    }
    return 0;
}


int main(){
    int v[1000000];
    int n, k;
    int acharv[30];
    int tempos[30];
    float sum = 0;
    preenche_vetor_aleatorio(v,100000);
    /*questao01*/
    for(int i = 0;i < 30;i++){
        if(i < 15){
            n = rand();
            k=n;
        }
        else{
            n = rand() % 1000000;
            k=v[n];
        }
        clock_t tempoini = clock();
        int achado = busca_bin(v,1000000,k);
        clock_t tempof = clock();
        float tempo_tot = (float)(tempof - tempoini) / CLOCKS_PER_SEC;
        float sum += tempo_tot;
        tempos[i] = tempo_tot;
        //acharv[i] = achado;
    }

    //printf("%f");
    for(int i = 0;i < 30;i++){
        if(i < 15){
            n = rand();
            k=n;
        }
        else{
            n = rand() % 1000000;
            k=v[n];
        }
        clock_t tempoini = clock();
        int achado = busca_seq(v,1000000,k);
        clock_t tempof = clock();
        float tempo_tot = (float)(tempof - tempoini) / CLOCKS_PER_SEC;
        tempos[i] = tempo_tot;
        acharv[i] = achado;
    }

    /*questao02*/

}
